import java.util.Scanner;
public class Q3
{
  public static void main (String [] args)
  {
    Scanner sc = new Scanner(System.in);
    System.out.println("Enter value of s");
    int s=sc.nextInt();
    double l;
    double q;
    q=4+(s*s/14900);
    if(s<100)
    {
      l=3000-125*s*s;
    }
    else 
    { 
      l=12000/q;
    }
    System.out.println("L="+q);
  }
}