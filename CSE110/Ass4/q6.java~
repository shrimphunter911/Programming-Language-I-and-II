import java.util.Scanner;
public class q6
{
  public static void main (String []args)
  {
    Scanner sc= new Scanner(System.in);
    System.out.println("A");
    int a=sc.nextInt();
    System.out.println("B");
    int b=sc.nextInt();
    int temp=a;
    a=b;
    b=temp;
    System.out.println(a);
    System.out.println(b);
  }
}